{
  "name": "@marktext/muya",
  "version": "0.1.13",
  "description": "Future markdown editor for web and desktop application development",
  "main": "dist/muya.js",
  "scripts": {
    "dev": "webpack serve --config ./example/webpack.config.js --open",
    "build": "npm run lint:tsc && webpack --config scripts/webpack.config.js",
    "test": "jest --env=jsdom",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:eslint": "eslint lib",
    "lint:tsc": "tsc --noEmit --skipLibCheck",
    "lint:fix": "eslint --fix lib",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "pub": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marktext/muya.git"
  },
  "keywords": [
    "markdown",
    "editor",
    "web",
    "desktop"
  ],
  "author": "ransixi@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marktext/muya/issues"
  },
  "homepage": "https://github.com/marktext/muya#readme",
  "dependencies": {
    "@marktext/file-icons": "^1.0.4",
    "dompurify": "^3.0.5",
    "element-resize-detector": "^1.2.4",
    "execall": "^3.0.0",
    "fast-diff": "^1.3.0",
    "flowchart.js": "^1.17.1",
    "fuse.js": "^6.6.2",
    "github-markdown-css": "^5.2.0",
    "html-tags": "^3.3.1",
    "intl-segmenter-polyfill": "^0.4.4",
    "joplin-turndown-plugin-gfm": "^1.0.12",
    "katex": "0.16.8",
    "mermaid": "10.4.0",
    "ot-json1": "^1.0.2",
    "ot-text-unicode": "^4.0.0",
    "popper.js": "^1.16.1",
    "prismjs": "^1.29.0",
    "snabbdom": "^3.5.1",
    "snabbdom-to-html": "^7.1.0",
    "snapsvg-cjs": "^0.0.6",
    "turndown": "^7.1.2",
    "underscore": "^1.13.6",
    "unsplash-js": "^7.0.18",
    "vega": "^5.25.0",
    "vega-embed": "^6.22.2",
    "vega-lite": "^5.15.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "7.22.19",
    "@babel/helper-string-parser": "^7.22.5",
    "@babel/preset-env": "7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@types/d3": "^7.4.0",
    "@types/dompurify": "^3.0.2",
    "@types/prismjs": "^1.26.0",
    "@types/vfile-message": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "browserify-zlib": "^0.2.0",
    "commitlint": "^17.7.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.49.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "8.0.3",
    "imports-loader": "^4.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
